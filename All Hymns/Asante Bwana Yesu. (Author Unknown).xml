<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4" modifiedIn="OpenLP 2.4" modifiedDate="2016-10-15T21:42:59">
  <properties>
    <titles>
      <title>Asante Bwana Yesu.</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Asante Bwana Yesu, kwa mema yako,kwa mema yako, uliyotujalia,</lines>
    </verse>
    <verse name="v2">
      <lines>Asante, kwa mema yako, kwa mema yako, uliyotujalia.</lines>
    </verse>
    <verse name="v3">
      <lines>1.Umetulisha umetunywesha twashukuru</lines>
    </verse>
    <verse name="v4">
      <lines>2.Mema  ya mbingu tumeyapata, twashukuru</lines>
    </verse>
    <verse name="v5">
      <lines>3.Mwana wa Mungu, u kati yetu</lines>
    </verse>
    <verse name="v6">
      <lines>4.Kwa vitu vyote ulivyotupa</lines>
    </verse>
    <verse name="v7">
      <lines>5.Msamaha wako umetujalia</lines>
    </verse>
    <verse name="v8">
      <lines>6.Asante sana kwa mema yote.</lines>
    </verse>
  </lyrics>
</song>
