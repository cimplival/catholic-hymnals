    <titles>
      <title>This Is My Commandment (Gospel Acclamation)</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>Peace</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Allelluya, Allelluya, <br/>This is my commandment,<br/>that you love one another,<br/>that your joy may be full,<br/>Allelluya, Allelluya, Allell-uya.</lines>
    </verse>
  </lyrics>
